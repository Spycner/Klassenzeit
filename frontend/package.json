{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "tsc -b && vite build && open dist/bundle-stats.html",
    "preview": "vite preview",
    "lint": "biome lint .",
